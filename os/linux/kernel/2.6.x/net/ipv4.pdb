<patterndb version='4' pub_date='2013-08-19'>

<ruleset name='kernel.net.ipv4' id='fa9797a7-fc8c-4109-a345-47dee1721d42'>
	<patterns>
		<pattern>kernel</pattern>
	</patterns>
	<rules>
		<rule provider='wernli@in2p3.fr' id='f65e1cef-677a-444c-b483-567485a72524'
			class='system'
			context-id='kernel.net.ipv4.route'
			context-scope='program'>
			<patterns>
				<pattern>kernel: ll header: @LLADDR:krnacct.route_llhdr@</pattern>
			</patterns>
			<examples>
				<example>
					<test_message program='kernel'>kernel: ll header: 00:22:19:65:b9:3a:00:26:98:24:9e:c1:08:00</test_message>
					<test_values>
						<test_value name='krnacct.route_llhdr'>00:22:19:65:b9:3a:00:26:98:24:9e:c1:08:00</test_value>
					</test_values>
				</example>
			</examples>
			<values>
				<value name="krnacct.name">Linux</value>
				<value name="krnacct.rsclass">net</value>
				<value name="krnacct.rstype">ipv4</value>
			</values>
			<tags>
				<tag>krnacct</tag>
			</tags>
		</rule>
		<rule provider='wernli@in2p3.fr' id='572e9893-e4c2-4076-8c17-74c70b2549a5'
			class='system'
			context-id='kernel.net.ipv4.route'
			context-scope='program'>
			<patterns>
				<pattern>kernel: martian source @IPvANY:flowevt.dst_ip@ from @IPvANY:flowevt.src_ip@, on dev @ANYSTRING:krnacct.rsname@</pattern>
			</patterns>
			<examples>
				<example>
					<test_message program='kernel'>kernel: martian source 10.0.106.201 from 10.0.69.167, on dev eth0</test_message>
					<test_values>
						<test_value name='flowevt.dst_ip'>10.0.106.201</test_value>
						<test_value name='flowevt.src_ip'>10.0.69.167</test_value>
						<test_value name='krnacct.rsname'>eth0</test_value>
					</test_values>
				</example>
			</examples>
			<values>
				<value name="krnacct.name">Linux</value>
				<value name="krnacct.rsclass">net</value>
				<value name="krnacct.rstype">ipv4</value>
			</values>
			<tags>
				<tag>krnacct</tag>
				<tag>flowevt</tag>
			</tags>
		</rule>
	</rules>
</ruleset>

</patterndb>

