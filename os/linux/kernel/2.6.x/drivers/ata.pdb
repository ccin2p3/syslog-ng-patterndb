<patterndb version='4' pub_date='2013-08-19'>

<ruleset name='kernel.drivers.ata' id='6025649b-6d0a-4903-886e-5e2ca22d745b'>
	<patterns>
		<pattern>kernel</pattern>
	</patterns>
	<rules>
		<rule provider="wernli@in2p3.fr" id='4b692672-4292-4112-84c4-c22e0f9af562'
			class='system'
			context-id='${krnacct.rsid}.${krnacct.rsclass}.${krnacct.rstype}.${krnacct.rsname}'
			context-timeout='10'
			context-scope='program'>
			<patterns>
				<!-- drivers/ata/libata-eh.c: ehc->i.dev, KERN_ERR, "exception Emask 0x%x SAct 0x%x SErr 0x%x action 0x%x%s%s\n",
				     ehc->i.err_mask, link->sactive, ehc->i.serror, ehc->i.action, frozen, tries_buf -->
				<pattern>kernel: ata@ESTRING:krnacct.rsid::@ exception Emask @NUMBER:krnacct.err_mask@ SAct @NUMBER:krnacct.sactive@ SErr @NUMBER:krnacct.serror@ action @NUMBER:krnacct.action@</pattern>
			</patterns>
			<examples>
				<example>
					<test_message program='kernel'>kernel: ata2.00: exception Emask 0x0 SAct 0xffff SErr 0x0 action 0x0</test_message>
					<test_values>
						<test_value name="krnacct.rsname">ata2.00</test_value>
						<test_value name="krnacct.rsid">2.00</test_value>
						<test_value name="krnacct.err_mask">0x0</test_value>
						<test_value name="krnacct.sactive">0xffff</test_value>
						<test_value name="krnacct.serror">0x0</test_value>
						<test_value name="krnacct.action">0x0</test_value>
					</test_values>
				</example>
			</examples>
			<values>
				<value name="krnacct.name">Linux</value>
				<value name="krnacct.rsname">ata${krnacct.rsid}</value>
				<value name="krnacct.rsclass">drivers</value>
				<value name="krnacct.rstype">ata-dev</value>
			</values>
			<tags>
				<tag>krnacct</tag><tag>hwevt</tag>
			</tags>
			<actions>
				<action condition='level(err..emerg)' rate='1/600' trigger='timeout'>
					<message inherit-properties="TRUE">
						<tags><tag>alert</tag></tags>
					</message>
				</action>
			</actions>
		</rule>
		<rule provider="wernli@in2p3.fr" id='cd9efb2f-2773-490d-a0ce-0328cceb24bd'
			class='system'
			context-id='${krnacct.rsclass}.${krnacct.rstype}.${krnacct.rsname}'
			context-timeout='10'
			context-scope='program'>
			<patterns>
				<!-- drivers/ata/libata-eh.c:  -->
				<pattern>kernel: ata@ESTRING:krnacct.rsid::@ status: { @ESTRING:krnacct.error: }@</pattern>
				<pattern>kernel: ata@ESTRING:krnacct.rsid::@ error: { @ESTRING:krnacct.error: }@</pattern>
			</patterns>
			<examples>
				<example>
					<test_message program='kernel'>kernel: ata2.00: status: { DRDY ERR }</test_message>
					<test_values>
						<test_value name="krnacct.rsid">2.00</test_value>
						<test_value name="krnacct.rsname">ata2.00</test_value>
						<test_value name="krnacct.error">DRDY ERR</test_value>
					</test_values>
				</example>
				<example>
					<test_message program='kernel'>kernel: ata2.00: status: { UNC }</test_message>
					<test_values>
						<test_value name="krnacct.rsid">2.00</test_value>
						<test_value name="krnacct.rsname">ata2.00</test_value>
						<test_value name="krnacct.error">UNC</test_value>
					</test_values>
				</example>
				<example>
					<test_message program='kernel'>kernel: ata2.00: error: { UNC }</test_message>
					<test_values>
						<test_value name="krnacct.rsid">2.00</test_value>
						<test_value name="krnacct.rsname">ata2.00</test_value>
						<test_value name="krnacct.error">UNC</test_value>
					</test_values>
				</example>
			</examples>
			<values>
				<value name="krnacct.name">Linux</value>
				<value name="krnacct.rsname">ata${krnacct.rsid}</value>
				<value name="krnacct.rsclass">drivers</value>
				<value name="krnacct.rstype">ata-dev</value>
			</values>
			<tags>
				<tag>krnacct</tag><tag>hwevt</tag><tag>alert</tag>
			</tags>
			<actions>
				<action condition='level(err..emerg)' rate='1/600' trigger='timeout'>
					<message inherit-properties="TRUE">
						<tags><tag>alert</tag></tags>
					</message>
				</action>
			</actions>
		</rule>
		<rule provider="wernli@in2p3.fr" id='9cfad9a4-aae4-49aa-b8cd-74b720b83036'
			class='system'
			context-id='${krnacct.rsclass}.${krnacct.rstype}.${krnacct.rsname}'
			context-timeout='10'
			context-scope='program'>
			<patterns>
				<pattern>kernel: ata@ESTRING:krnacct.rsid::@ cmd @ESTRING:krnacct.res: @tag @NUMBER:krnacct.tag@ @ANYSTRING:krnacct.desc@</pattern>
				<pattern>kernel:@SET:: @res @ESTRING:krnacct.res: @Emask @NUMBER:krnacct.err_mask@ (@ESTRING:krnacct.error:)@</pattern>
			</patterns>
			<examples>
				<example>
					<test_message program='kernel'>kernel: ata2.00: cmd 61/10:18:88:81:0f/00:00:04:00:00/40 tag 3 ncq 8192 out</test_message>
					<test_values>
						<test_value name="krnacct.rsid">2.00</test_value>
						<test_value name="krnacct.res">61/10:18:88:81:0f/00:00:04:00:00/40</test_value>
						<test_value name="krnacct.tag">3</test_value>
						<test_value name="krnacct.desc">ncq 8192 out</test_value>
					</test_values>
				</example>
				<example>
					<test_message program='kernel'>kernel: ata2.00: cmd 25/00:10:90:41:1d/00:00:31:00:00/e0 tag 0 dma 8192 in</test_message>
					<test_values>
						<test_value name="krnacct.rsid">2.00</test_value>
						<test_value name="krnacct.res">25/00:10:90:41:1d/00:00:31:00:00/e0</test_value>
						<test_value name="krnacct.tag">0</test_value>
						<test_value name="krnacct.desc">dma 8192 in</test_value>
					</test_values>
				</example>
				<example>
					<test_message program='kernel'>kernel:         res 41/40:00:00:00:00/00:00:00:00:00/00 Emask 0x9 (media error)</test_message>
					<test_values>
						<test_value name="krnacct.res">41/40:00:00:00:00/00:00:00:00:00/00</test_value>
						<test_value name="krnacct.err_mask">0x9</test_value>
						<test_value name="krnacct.error">media error</test_value>
					</test_values>
				</example>
				<example>
					<test_message program='kernel'>kernel: res 51/40:00:90:41:1d/b4:00:31:00:00/e0 Emask 0x9 (media error)</test_message>
					<test_values>
						<test_value name="krnacct.res">51/40:00:90:41:1d/b4:00:31:00:00/e0</test_value>
						<test_value name="krnacct.err_mask">0x9</test_value>
						<test_value name="krnacct.error">media error</test_value>
					</test_values>
				</example>
			</examples>
			<values>
				<value name="krnacct.name">Linux</value>
				<value name="krnacct.rsname">ata${krnacct.rsid}</value>
				<value name="krnacct.rsclass">drivers</value>
				<value name="krnacct.rstype">ata-dev</value>
			</values>
			<tags>
				<tag>krnacct</tag><tag>hwevt</tag>
			</tags>
			<actions>
				<action condition='level(err..emerg)' rate='1/600' trigger='timeout'>
					<message inherit-properties="TRUE">
						<tags><tag>alert</tag></tags>
					</message>
				</action>
			</actions>
		</rule>
		<rule provider="wernli@in2p3.fr" id='7cb3b123-1241-4cfb-ac10-55cfe1d027ca'
			class='system'
			context-id='${krnacct.rsclass}.${krnacct.rstype}.${krnacct.rsname}'
			context-timeout='10'
			context-scope='program'>
			<patterns>
				<pattern>kernel: ata@ESTRING:krnacct.rsid::@ irq_stat @NUMBER:krnacct.irq_stat@</pattern>
			</patterns>
			<examples>
				<example>
					<test_message program='kernel'>kernel: ata2.00: irq_stat 0x40000001</test_message>
					<test_values>
						<test_value name="krnacct.rsid">2.00</test_value>
						<test_value name="krnacct.irq_stat">0x40000001</test_value>
					</test_values>
				</example>
			</examples>
			<tags>
				<tag>kernel</tag><tag>drivers</tag><tag>ata</tag><tag>hardware</tag>
			</tags>
			<values>
				<value name=".classifier.context-id">$CONTEXT_ID</value>
			</values>
		</rule>
		<rule provider="wernli@in2p3.fr" id='52246cc5-86c6-401d-bf35-d5f209ec0669'
			class='system'
			context-id='${krnacct.rsclass}.${krnacct.rstype}.${krnacct.rsname}'
			context-timeout='10'
			context-scope='program'>
			<patterns>
				<pattern>kernel: ata@ESTRING:krnacct.rsid::@ failed command: @ANYSTRING:krnacct.cmd@</pattern>
			</patterns>
			<examples>
				<example>
					<test_message program='kernel'>kernel: ata1.00: failed command: READ FPDMA QUEUED</test_message>
					<test_values>
						<test_value name="krnacct.rsid">1.00</test_value>
						<test_value name="krnacct.cmd">READ FPDMA QUEUED</test_value>
					</test_values>
				</example>
			</examples>
			<values>
				<value name="krnacct.name">Linux</value>
				<value name="krnacct.rsname">ata${krnacct.rsid}</value>
				<value name="krnacct.rsclass">drivers</value>
				<value name="krnacct.rstype">ata-dev</value>
			</values>
			<tags>
				<tag>krnacct</tag><tag>hwevt</tag>
			</tags>
		</rule>
		<rule provider="wernli@in2p3.fr" id='656ba7a8-c79a-4a20-8437-03ff0c247ff0'
			class='system'
			context-id='${krnacct.rsclass}.${krnacct.rstype}.${krnacct.rsname}'
			context-scope='program'>
			<patterns>
				<pattern>kernel: ata@ESTRING:krnacct.rsid::@ SError: { @ESTRING:krnacct.serror: }@</pattern>
			</patterns>
			<examples>
				<example>
					<test_message program='kernel'>kernel: ata1: SError: { UnrecovData HostInt 10B8B BadCRC }</test_message>
					<test_values>
						<test_value name="krnacct.rsid">1</test_value>
						<test_value name="krnacct.serror">UnrecovData HostInt 10B8B BadCRC</test_value>
					</test_values>
				</example>
			</examples>
			<values>
				<value name="krnacct.name">Linux</value>
				<value name="krnacct.rsname">ata${krnacct.rsid}</value>
				<value name="krnacct.rsclass">drivers</value>
				<value name="krnacct.rstype">ata-link</value>
			</values>
			<tags>
				<tag>krnacct</tag><tag>hwevt</tag>
			</tags>
			<actions>
				<action condition='level(err..emerg)' rate='1/600' trigger='timeout'>
					<message inherit-properties="TRUE">
						<tags><tag>alert</tag></tags>
					</message>
				</action>
			</actions>
		</rule>
		<rule provider='wernli@in2p3.fr' id='f9d016cf-f4c3-42f4-ae0d-64435cd8e7fa'
			class='system'
			context-id='${krnacct.rsclass}.${krnacct.rstype}.${krnacct.rsname}'
		context-timeout='30'
			context-scope='program'>
			<patterns>
				<!-- drivers/ata/libata-eh.c: ata_link_printk(link, KERN_ERR, "failed to read log page 10h "
								        "(errno=%d)\n", rc); -->
				<pattern>kernel: ata@ESTRING:krnacct.rsid::@ failed to read log page 10h (errno=@NUMBER:krnacct.errno:)@</pattern>
			</patterns>
			<examples>
				<example>
					<test_message program='kernel'>kernel: ata1: failed to read log page 10h (errno=-5)</test_message>
					<test_values>
						<test_value name="krnacct.rsid">1</test_value>
						<test_value name="krnacct.errno">-5</test_value>
					</test_values>
				</example>
			</examples>
			<values>
				<value name="krnacct.name">Linux</value>
				<value name="krnacct.rsname">ata${krnacct.rsid}</value>
				<value name="krnacct.rsclass">drivers</value>
				<value name="krnacct.rstype">ata-link</value>
			</values>
			<tags>
				<tag>krnacct</tag><tag>hwevt</tag>
			</tags>
			<actions>
				<action condition='level(err..emerg)' rate='1/600' trigger='timeout'>
					<message inherit-properties="TRUE">
						<tags><tag>alert</tag></tags>
					</message>
				</action>
			</actions>
	</rule>
	<rule provider='wernli@in2p3.fr' id='6c733e8c-ec5c-4a14-9df7-3a9ce429ddfe'
		class='system'
		context-id='${krnacct.rsclass}.${krnacct.rstype}.${krnacct.rsname}'
		context-timeout='30'
		context-scope='program'>
		<patterns>
			<!-- drivers/ata/libata-core.c:    ata_link_printk(link, KERN_ERR,
						         "failed to resume link (SControl %X)\n", scontrol);
				  -->
			<pattern>kernel: ata@ESTRING:krnacct.rsid::@ failed to resume link (SControl @NUMBER:krnacct.scontrol:)@</pattern>
		</patterns>
		<examples>
			<example>
				<test_message program='kernel'>kernel: ata2: failed to resume link (SControl 0)</test_message>
				<test_values>
					<test_value name="krnacct.rsid">2</test_value>
					<test_value name="krnacct.scontrol">0</test_value>
				</test_values>
			</example>
		</examples>
		<values>
			<value name="krnacct.name">Linux</value>
			<value name="krnacct.rsname">ata${krnacct.rsid}</value>
			<value name="krnacct.rsclass">drivers</value>
			<value name="krnacct.rstype">ata-link</value>
		</values>
		<tags>
			<tag>krnacct</tag><tag>hwevt</tag>
		</tags>
		<actions>
			<action condition='level(err..emerg)' rate='1/600' trigger='timeout'>
				<message inherit-properties="TRUE">
					<tags><tag>alert</tag></tags>
				</message>
			</action>
		</actions>
	</rule>
	<rule provider='wernli@in2p3.fr' id='09e95500-ba17-4498-867b-38f9207ae8fb'
		class='system'
		context-id='${krnacct.rsclass}.${krnacct.rstype}.${krnacct.rsname}'
		context-timeout='30'
		context-scope='program'>
		<patterns>
			<!-- drivers/ata/libata-core.c: ata_link_printk(link, KERN_WARNING, "limiting SATA link speed to %s\n",
						      sata_spd_string(fls(mask)));
			-->
			<pattern>kernel: ata@ESTRING:krnacct.rsid::@ limiting SATA link speed to @ANYSTRING:krnacct.lspeed@</pattern>
		</patterns>
		<examples>
			<example>
				<test_message program='kernel'>kernel: ata1: limiting SATA link speed to 1.5 Gbps</test_message>
				<test_values>
					<test_value name="krnacct.rsid">1</test_value>
					<test_value name="krnacct.lspeed">1.5 Gbps</test_value>
				</test_values>
			</example>
		</examples>
		<values>
			<value name="krnacct.name">Linux</value>
			<value name="krnacct.rsname">ata${krnacct.rsid}</value>
			<value name="krnacct.rsclass">drivers</value>
			<value name="krnacct.rstype">ata-link</value>
		</values>
		<tags>
			<tag>krnacct</tag><tag>hwevt</tag>
		</tags>
			<actions>
				<action condition='level(err..emerg)' rate='1/600' trigger='timeout'>
					<message inherit-properties="TRUE">
						<tags><tag>alert</tag></tags>
					</message>
				</action>
			</actions>
	</rule>
</rules>
</ruleset>

</patterndb>

