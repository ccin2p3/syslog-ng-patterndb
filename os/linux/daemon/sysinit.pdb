<patterndb version='4' pub_date='2013-08-27'>

<ruleset name='sysvinit' id='382b596b-3923-40b7-bd1f-45842dbdefa4'>
	<patterns>
		<pattern>init</pattern>
	</patterns>
	<rules>
		<rule provider="wernli@in2p3.fr" id='75035a2d-44f7-4327-a1a1-61b13cac4fba'
			class='system'
			context-id='system.init'
			context-scope="program">
			<patterns>
				<pattern>@ESTRING:system.init.console.type: (@@ESTRING:system.init.console.path:)@ main process (@NUMBER:system.init.console.pid@) killed by @STRING:system.init.console.signal@ signal</pattern>
			</patterns>
			<examples>
				<example>
					<test_message program="init">tty (/dev/tty6) main process (12900) killed by TERM signal</test_message>
					<test_values>
						<test_value name="system.init.console.type">tty</test_value>
						<test_value name="system.init.console.path">/dev/tty6</test_value>
						<test_value name="system.init.console.pid">12900</test_value>
						<test_value name="system.init.console.signal">TERM</test_value>
					</test_values>
				</example>
				<example>
					<test_message program="init">serial (ttyS0) main process (19802) killed by TERM signal</test_message>
					<test_values>
						<test_value name="system.init.console.type">serial</test_value>
						<test_value name="system.init.console.path">ttyS0</test_value>
						<test_value name="system.init.console.pid">19802</test_value>
						<test_value name="system.init.console.signal">TERM</test_value>
					</test_values>
				</example>
			</examples>
			<tags>
				<tag>alert</tag>
			</tags>
		</rule>
	</rules>
</ruleset>

</patterndb>

