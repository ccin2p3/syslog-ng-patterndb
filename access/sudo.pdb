<?xml version='1.0' encoding='UTF-8'?>
<patterndb version='4' pub_date='2010-07-13'>
  <ruleset name='sudo' id='1f5fd75e-66f9-4598-8326-36e5bbd5675c'>
    <description>
      This ruleset covers the sudo command available on UNIXes.
    </description>
    <pattern>sudo</pattern>
    <rules>

      <rule provider="patterndb" id="ed8a54b4-d272-4bfb-a523-1eccd6f3fc7c"
      	context-id='sudo-command-${temp.sudo_username}'
      	context-timeout='1'
      	class="system">
        <!-- login continued -->
        <patterns>
          <pattern>@ESTRING:temp.sudo_username: @: (command continued) @ANYSTRING:temp.sudo_command@</pattern>
          <pattern>@SET:: @@ESTRING:temp.sudo_username: @: (command continued) @ANYSTRING:temp.sudo_command@</pattern>
        </patterns>
        <examples>
          <example>
            <test_message program='sudo'> atlas : (command continued) ich/is/being/continued/on/the/next/line</test_message>
            <test_values>
              <test_value name='usracct.username'>root</test_value>
              <test_value name='usracct.device'>atlas@unknown</test_value>
              <test_value name='usracct.object'>/home/atlas$ /usr/bin/unlink /very/long/path/to/some/place/which/is/being/continued/on/the/next/line</test_value>
            </test_values>
          </example>
        </examples>
        <values>
          <value name="usracct.type">login</value>
          <value name="usracct.username">${usracct.username}@1</value>
          <value name="usracct.sessionid">$PID</value>
          <value name="usracct.application">$PROGRAM</value>
          <value name="usracct.device">${usracct.device}@1</value>
          <value name="usracct.object">${usracct.object}@1${temp.sudo_command}</value>
          <value name="secevt.verdict">ACCEPT</value>
        </values>
      </rule>

      <rule provider="patterndb" id="ed8a54b4-d272-4bfb-a523-1eccd6f3fc7c"
      	context-id='sudo-command-${temp.sudo_username}'
      	context-timeout='1'
      	class="system">
        <!-- login success -->
        <patterns>
          <!-- sudo logs the username right-aligned to 8 chars -->
          <pattern>@ESTRING:temp.sudo_username: @: HOST=@ESTRING:temp.sudo_host: @: TTY=@ESTRING:temp.sudo_tty: @; PWD=@ESTRING:temp.sudo_pwd: ;@ USER=@ESTRING:usracct.username: ;@ COMMAND=@ANYSTRING:temp.sudo_command@</pattern>
          <pattern>@ESTRING:temp.sudo_username: @: TTY=@ESTRING:temp.sudo_tty: @; PWD=@ESTRING:temp.sudo_pwd: ;@ USER=@ESTRING:usracct.username: ;@ COMMAND=@ANYSTRING:temp.sudo_command@</pattern>
          <pattern>@SET:: @@ESTRING:temp.sudo_username: @: TTY=@ESTRING:temp.sudo_tty: @; PWD=@ESTRING:temp.sudo_pwd: ;@ USER=@ESTRING:usracct.username: ;@ COMMAND=@ANYSTRING:temp.sudo_command@</pattern>
          <pattern>@SET:: @@ESTRING:temp.sudo_username: @: HOST=@ESTRING:temp.sudo_host: @: TTY=@ESTRING:temp.sudo_tty: @; PWD=@ESTRING:temp.sudo_pwd: ;@ USER=@ESTRING:usracct.username: ;@ COMMAND=@ANYSTRING:temp.sudo_command@</pattern>
        </patterns>
        <examples>
          <example>
            <test_message program="sudo">  phemmer : HOST=admin02 : TTY=pts/13 ; PWD=/home/phemmer ; USER=root ; COMMAND=/bin/ls</test_message>
            <test_values>
              <test_value name="usracct.username">root</test_value>
              <test_value name="usracct.device">phemmer@pts/13</test_value>
              <test_value name="usracct.object">/home/phemmer$ /bin/ls</test_value>
           </test_values>
          </example>
          <example>
            <test_message program="sudo">atlas : TTY=unknown ; PWD=/home/atlas ; USER=root ; COMMAND=/usr/bin/unlink /very/long/path/to/some/place/wh</test_message>
            <test_values>
              <test_value name="usracct.username">root</test_value>
              <test_value name="usracct.device">atlas@unknown</test_value>
              <test_value name="usracct.object">/home/atlas$ /usr/bin/unlink /very/long/path/to/some/place/wh</test_value>
           </test_values>
          </example>
        </examples>
        <values>
          <value name="usracct.type">login</value>
          <value name="usracct.sessionid">$PID</value>
          <value name="usracct.application">$PROGRAM</value>
          <value name="usracct.device">${temp.sudo_username}@@${temp.sudo_tty}</value>
          <value name="usracct.object">${temp.sudo_pwd}$$ ${temp.sudo_command}</value>
          <value name="secevt.verdict">ACCEPT</value>
        </values>
        <tags>
          <tag>usracct</tag>
          <tag>secevt</tag>
        </tags>
      </rule>
      
      <rule provider="patterndb" id="693a8131-6a03-423a-9dc4-a310591b3420" class="system">
        <!-- login failure -->
        <patterns>
          <pattern>@ESTRING:temp.sudo_username: @: no passwd entry for @ESTRING:usracct.username:!@</pattern>
        </patterns>
        <examples>
          <example>
           <test_message program="sudo">phemmer : no passwd entry for asdfh!</test_message>
           <test_values>
             <test_value name="usracct.username">asdfh</test_value>
             <test_value name="usracct.device">phemmer@unknown</test_value>
           </test_values>
          </example>
        </examples>
        <values>
          <value name="usracct.type">login</value>
          <value name="usracct.sessionid">$PID</value>
          <value name="usracct.application">$PROGRAM</value>
          <value name="usracct.device">${temp.sudo_username}@@unknown</value>
          <value name="secevt.verdict">REJECT</value>
        </values>
        <tags>
          <tag>usracct</tag>
          <tag>secevt</tag>
        </tags>
      </rule>

    </rules>
  </ruleset>
</patterndb>
