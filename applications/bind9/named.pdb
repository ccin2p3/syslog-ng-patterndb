<?xml version='1.0' encoding='utf-8'?>
<patterndb version='3' pub_date='2013-02-26'>
  <ruleset name="named" id="f1f9b113-cf9f-4658-b0ab-38998ba5205c">
    <description>Messages from cron named (bind9). This assumes there is no timestamp and severity turned on. The category prefix is supported.</description>
    <pattern>named</pattern>
    <rules>
      <rule provider="balabit" id="4fadbeff-65e4-4d83-95fa-d89dc21c8610" class="system">
        <patterns>
          <pattern>client @IPvANY:named.client.ip@#@NUMBER:named.client.port@: query (cache) '@ESTRING:named.domain.name:/@@ESTRING:named.record.type:/@IN' denied</pattern>
          <pattern>security: client @IPvANY:named.client.ip@#@NUMBER:named.client.port@: query (cache) '@ESTRING:named.domain.name:/@@ESTRING:named.record.type:/@IN' denied</pattern>
        </patterns>
        <examples>
          <example>
            <test_message program="named">client 35.55.71.17#26416: query (cache) 'qe-tdft.com/MX/IN' denied</test_message>
            <test_values>
              <test_value name="named.client.ip">35.55.71.17</test_value>
              <test_value name="named.client.port">26416</test_value>
              <test_value name="named.domain.name">qe-tdft.com</test_value>
              <test_value name="named.record.type">MX</test_value>
            </test_values>
          </example>
        </examples>
        <values>
        </values>
        <tags>
          <tag>named</tag>
          <tag>security</tag>
        </tags>
      </rule>
      <rule provider="balabit" id="9847c997-79b1-4ec5-8633-b92ba3a726d5" class="system">
        <patterns>
          <pattern>client @IPvANY:named.client.ip@#@NUMBER:named.client.port@: RFC 1918 response from Internet for @ANYSTRING:named.domain.name@</pattern>
          <pattern>client @IPvANY:named.client.ip@#@NUMBER:named.client.port@: view @ESTRING:named.zone.view::@ RFC 1918 response from Internet for @ANYSTRING:named.domain.name@</pattern>
          <pattern>security: client @IPvANY:named.client.ip@#@NUMBER:named.client.port@: RFC 1918 response from Internet for @ANYSTRING:named.domain.name@</pattern>
          <pattern>security: client @IPvANY:named.client.ip@#@NUMBER:named.client.port@: view @ESTRING:named.zone.view::@ RFC 1918 response from Internet for @ANYSTRING:named.domain.name@</pattern>
        </patterns>
        <examples>
          <example>
            <test_message program="named">client 127.0.0.1#39550: RFC 1918 response from Internet for 183.25.168.192.in-addr.arpa</test_message>
            <test_values>
              <test_value name="named.client.ip">127.0.0.1</test_value>
              <test_value name="named.client.port">39550</test_value>
              <test_value name="named.domain.name">183.25.168.192.in-addr.arpa</test_value>
            </test_values>
          </example>
          <example>
            <test_message program="named">security: client 127.0.0.1#54316: view internal: RFC 1918 response from Internet for 2.49.16.172.in-addr.arpa</test_message>
            <test_values>
              <test_value name="named.client.ip">127.0.0.1</test_value>
              <test_value name="named.client.port">54316</test_value>
              <test_value name="named.zone.view">internal</test_value>
              <test_value name="named.domain.name">2.49.16.172.in-addr.arpa</test_value>
            </test_values>
          </example>
        </examples>
        <values>
        </values>
        <tags>
          <tag>named</tag>
          <tag>security</tag>
        </tags>
      </rule>
      <rule provider="balabit" id="904d5acc-096b-4ead-be9d-5da7360ab1d5" class="system">
        <patterns>
          <pattern>DNS format error from @IPvANY:named.server.ip@#@NUMBER:named.server.port@ resolving @ESTRING:named.domain.name:/@@ESTRING:named.record.type: @for client @IPvANY:named.client.ip@#@NUMBER:named.client.port@: @ANYSTRING:named.error.msg@</pattern>
          <pattern>DNS format error from @IPvANY:named.server.ip@#@NUMBER:named.server.port@ resolving @ESTRING:named.domain.name:/@@ESTRING:named.record.type::@ @ANYSTRING:named.error.msg@</pattern>
          <pattern>resolver: DNS format error from @IPvANY:named.server.ip@#@NUMBER:named.server.port@ resolving @ESTRING:named.domain.name:/@@ESTRING:named.record.type: @for client @IPvANY:named.client.ip@#@NUMBER:named.client.port@: @ANYSTRING:named.error.msg@</pattern>
          <pattern>resolver: DNS format error from @IPvANY:named.server.ip@#@NUMBER:named.server.port@ resolving @ESTRING:named.domain.name:/@@ESTRING:named.record.type::@ @ANYSTRING:named.error.msg@</pattern>
        </patterns>
        <examples>
          <example>
            <test_message program="named">DNS format error from 103.5.198.211#53 resolving resolver.1.geo.ctmail.com/AAAA for client 127.0.0.1#57551: invalid response</test_message>
            <test_values>
              <test_value name="named.server.ip">103.5.198.211</test_value>
              <test_value name="named.server.port">53</test_value>
              <test_value name="named.domain.name">resolver.1.geo.ctmail.com</test_value>
              <test_value name="named.record.type">AAAA</test_value>
              <test_value name="named.client.ip">127.0.0.1</test_value>
              <test_value name="named.client.port">57551</test_value>
              <test_value name="named.error.msg">invalid response</test_value>
            </test_values>
          </example>
          <example>
            <test_message program="named">DNS format error from 207.109.251.101#53 resolving ns2.visp.net/AAAA: non-improving referral</test_message>
            <test_values>
              <test_value name="named.server.ip">207.109.251.101</test_value>
              <test_value name="named.server.port">53</test_value>
              <test_value name="named.domain.name">ns2.visp.net</test_value>
              <test_value name="named.record.type">AAAA</test_value>
              <test_value name="named.error.msg">non-improving referral</test_value>
            </test_values>
          </example>
          <example>
            <test_message program="named">resolver: DNS format error from 217.20.130.3#53 resolving assets.index.hu/A for client 192.168.102.2#23188: sideways referral</test_message>
            <test_values>
              <test_value name="named.server.ip">217.20.130.3</test_value>
              <test_value name="named.server.port">53</test_value>
              <test_value name="named.domain.name">assets.index.hu</test_value>
              <test_value name="named.record.type">A</test_value>
              <test_value name="named.client.ip">192.168.102.2</test_value>
              <test_value name="named.client.port">23188</test_value>
              <test_value name="named.error.msg">sideways referral</test_value>
            </test_values>
          </example>
        </examples>
        <values>
        </values>
        <tags>
          <tag>named</tag>
          <tag>resolver</tag>
        </tags>
      </rule> 
      <rule provider="balabit" id="3fbe2e79-5944-42cd-b87b-91518f86e438" class="system">
        <patterns>
          <pattern>error @QSTRING:named.error.type:()@ resolving '@ESTRING:named.domain.name:/@@ESTRING:named.record.type:/@IN': @IPvANY:named.server.ip@#@NUMBER:named.server.port@</pattern>
          <pattern>resolver: error @QSTRING:named.error.type:()@ resolving '@ESTRING:named.domain.name:/@@ESTRING:named.record.type:/@IN': @IPvANY:named.server.ip@#@NUMBER:named.server.port@</pattern>
        </patterns>
        <examples>
          <example>
            <test_message program="named">error (FORMERR) resolving 'resolver.1.geo.ctmail.com/AAAA/IN': 103.5.198.211#53</test_message>
            <test_values>
              <test_value name="named.error.type">FORMERR</test_value>
              <test_value name="named.domain.name">resolver.1.geo.ctmail.com</test_value>
              <test_value name="named.record.type">AAAA</test_value>
              <test_value name="named.server.ip">103.5.198.211</test_value>
              <test_value name="named.server.port">53</test_value>
            </test_values>
          </example>
          <example>
            <test_message program="named">error (network unreachable) resolving 'ns1.netidea.hu/AAAA/IN': 2001:678:4::c#53</test_message>
            <test_values>
              <test_value name="named.error.type">network unreachable</test_value>
              <test_value name="named.domain.name">ns1.netidea.hu</test_value>
              <test_value name="named.record.type">AAAA</test_value>
              <test_value name="named.server.ip">2001:678:4::c</test_value>
              <test_value name="named.server.port">53</test_value>
            </test_values>
          </example>
        </examples>
        <values>
        </values>
        <tags>
          <tag>named</tag>
          <tag>resolver</tag>
        </tags>
      </rule> 
      <rule provider="balabit" id="b0ca071d-309e-454a-8e31-721b01ef55ee" class="system">
        <patterns>
          <pattern>client @IPvANY:named.client.ip@#@NUMBER:named.client.port@: query: @ESTRING:named.domain.name: @IN @ESTRING:named.record.type: @@ESTRING:named.record.flags: @(@IPvANY:named.server.ip@)</pattern>
          <pattern>queries: client @IPvANY:named.client.ip@#@NUMBER:named.client.port@: query: @ESTRING:named.domain.name: @IN @ESTRING:named.record.type: @@ESTRING:named.record.flags: @(@IPvANY:named.server.ip@)</pattern>
        </patterns>
        <examples>
          <example>
            <test_message program="named">client 213.46.246.51#1890: query: tumtali.fegade.com IN A - (103.5.198.211)</test_message>
            <test_values>
              <test_value name="named.client.ip">213.46.246.51</test_value>
              <test_value name="named.client.port">1890</test_value>
              <test_value name="named.domain.name">tumtali.fegade.com</test_value>
              <test_value name="named.record.type">A</test_value>
              <test_value name="named.record.flags">-</test_value>
              <test_value name="named.server.ip">103.5.198.211</test_value>
            </test_values>
          </example>
          <example>
            <test_message program="named">client 10.187.195.186#40441: query: banister.paars.tld IN A -EDC (10.226.134.25)</test_message>
            <test_values>
              <test_value name="named.client.ip">10.187.195.186</test_value>
              <test_value name="named.client.port">40441</test_value>
              <test_value name="named.domain.name">banister.paars.tld</test_value>
              <test_value name="named.record.type">A</test_value>
              <test_value name="named.record.flags">-EDC</test_value>
              <test_value name="named.server.ip">10.226.134.25</test_value>
            </test_values>
          </example>
          <example>
            <test_message program="named">queries: client 192.168.102.189#58454: query: maps.googleapis.com IN A + (192.168.102.2)</test_message>
            <test_values>
              <test_value name="named.client.ip">192.168.102.189</test_value>
              <test_value name="named.client.port">58454</test_value>
              <test_value name="named.domain.name">maps.googleapis.com</test_value>
              <test_value name="named.record.type">A</test_value>
              <test_value name="named.record.flags">+</test_value>
              <test_value name="named.server.ip">192.168.102.2</test_value>
            </test_values>
          </example>
        </examples>
        <values>
        </values>
        <tags>
          <tag>named</tag>
          <tag>queries</tag>
        </tags>
      </rule> 
