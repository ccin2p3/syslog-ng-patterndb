<patterndb version='4' pub_date='2013-08-26'>

<ruleset name='portmap' id='19f9c156-9c4e-4bb7-9958-30400439cc0c'>
	<patterns>
		<pattern>portmap</pattern>
	</patterns>
	<rules>
		<rule provider="wernli@in2p3.fr" id='bc98fb8f-5725-4b0e-a594-16591e9bce4b'
			class='system.daemon'
			context-id='portmap'
			context-scope="process">
			<patterns>
				<pattern>connect from @IPvANY:flowevt.src_ip@ to callit(@ESTRING:appacct.rsclass:)@: @ANYSTRING:details@</pattern>
			</patterns>
			<examples>
				<example>
					<test_message program="portmap">connect from 10.0.105.226 to callit(ypserv): request from unauthorized host</test_message>
					<test_values>
						<test_value name="flowevt.src_ip">10.0.105.226</test_value>
						<test_value name="appacct.rsclass">ypserv</test_value>
						<test_value name="details">request from unauthorized host</test_value>
					</test_values>
				</example>
			</examples>
			<values>
				<value name="secevt.verdict">REJECT</value>
				<value name="appacct.name">$PROGRAM</value>
			</values>
			<tags>
				<tag>secevt</tag>
				<tag>flowevt</tag>
				<tag>appacct</tag>
			</tags>
		</rule>
	</rules>
</ruleset>

</patterndb>

