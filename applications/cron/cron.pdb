<patterndb version='4' pub_date='2013-08-26'>

<ruleset name='cron' id='3a8efec5-6974-4b4a-b203-3247fe235812'>
	<patterns>
		<pattern>crond</pattern>
		<pattern>CROND</pattern>
	</patterns>
	<rules>
		<rule provider="wernli@in2p3.fr" id='8a9cb5ac-6505-47d4-aef9-8cfc0c70c883'
			class='system.daemon'
			context-id="crond"
			context-scope="process">
			<patterns>
				<pattern>(@ESTRING:usracct.username:)@ @ESTRING:appacct.fct: (@@ESTRING:appacct.msg:)@</pattern>
			</patterns>
			<examples>
				<example>
					<test_message program="crond">(root) CMD (/usr/lib64/sa/sa1 1 1)</test_message>
					<test_values>
						<test_value name="usracct.username">root</test_value>
						<test_value name="appacct.fct">CMD</test_value>
						<test_value name="appacct.msg">/usr/lib64/sa/sa1 1 1</test_value>
					</test_values>
				</example>
				<example>
					<test_message program="crond">(*system*) RELOAD (/etc/cron.d/raid-check)</test_message>
					<test_values>
						<test_value name="usracct.username">*system*</test_value>
						<test_value name="appacct.fct">RELOAD</test_value>
						<test_value name="appacct.msg">/etc/cron.d/raid-check</test_value>
					</test_values>
				</example>
				<example>
					<test_message program="crond">(CRON) chdir(HOME) failed: (Permission denied)</test_message>
					<test_values>
						<test_value name="usracct.username">CRON</test_value>
						<test_value name="appacct.fct">chdir(HOME) failed:</test_value>
						<test_value name="appacct.msg">Permission denied</test_value>
					</test_values>
				</example>
				<example>
					<test_message program="crond">(root) INFO (Job execution of per-minute job scheduled for 14:08 delayed into subsequent minute 14:10. Skipping job run.)</test_message>
					<test_values>
						<test_value name="usracct.username">root</test_value>
						<test_value name="appacct.fct">INFO</test_value>
					</test_values>
				</example>
			</examples>
			<tags>
				<tag>usracct</tag>
				<tag>appacct</tag>
			</tags>
		</rule>
	</rules>
</ruleset>

</patterndb>

