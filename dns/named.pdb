<patterndb version='4' pub_date='2013-08-26'>

<ruleset name='named' id='db6b4f71-88f2-4e1b-a3d5-908a3da63225'>
	<patterns>
		<pattern>named</pattern>
	</patterns>
	<rules>
		<rule provider="wernli@in2p3.fr" id='bc98fb8f-5725-4b0e-a594-16591e9bce4b'
			class='system.daemon'
			context-id='named'
			context-scope="process">
			<patterns>
				<pattern>FORMERR resolving '@ESTRING:system.daemon.named.entry:'@: @IPvANY:system.daemon.named.nameserver.ip@#@NUMBER:system.daemon.named.nameserver.port@</pattern>
			</patterns>
			<examples>
				<example>
					<test_message program="named">FORMERR resolving 'my.fqdn.at.tld/AAAA/IN': 10.0.69.164#53</test_message>
					<test_values>
						<test_value name="system.daemon.named.entry">my.fqdn.at.tld/AAAA/IN</test_value>
						<test_value name="system.daemon.named.nameserver.ip">10.0.69.164</test_value>
						<test_value name="system.daemon.named.nameserver.port">53</test_value>
					</test_values>
				</example>
			</examples>
		</rule>
		<rule provider="wernli@in2p3.fr" id='75c57e45-0c0a-436d-bf84-1dd06876b718'
			class='system.daemon'
			context-id='named'
			context-scope="process">
			<patterns>
				<pattern>zone @ESTRING:system.daemon.named.zone::@ @ESTRING:system.daemon.named.entry::@ bad owner name (check-names)</pattern>
			</patterns>
			<examples>
				<example>
					<test_message program="named">zone my.zone.tld/IN: foo.bar.my.zone.tld/A: bad owner name (check-names)</test_message>
					<test_values>
						<test_value name="system.daemon.named.entry">foo.bar.my.zone.tld/A</test_value>
						<test_value name="system.daemon.named.zone">my.zone.tld/IN</test_value>
					</test_values>
				</example>
			</examples>
		</rule>
		<rule provider="wernli@in2p3.fr" id='f0c03ef7-f887-4046-97ec-809f1bccd844'
			class='system.daemon'
			context-id='named'
			context-scope="process">
			<patterns>
				<pattern>unexpected RCODE @QSTRING:system.daemon.named.rcode:()@ resolving '@ESTRING:system.daemon.named.entry:'@: @IPvANY:system.daemon.named.nameserver.ip@#@NUMBER:system.daemon.named.nameserver.port@</pattern>
			</patterns>
			<examples>
				<example>
					<test_message program="named">unexpected RCODE (REFUSED) resolving 'boro.cat.cbpf.br/A/IN': 152.84.253.220#53</test_message>
					<test_values>
						<test_value name="system.daemon.named.entry">boro.cat.cbpf.br/A/IN</test_value>
						<test_value name="system.daemon.named.rcode">REFUSED</test_value>
						<test_value name="system.daemon.named.nameserver.ip">152.84.253.220</test_value>
						<test_value name="system.daemon.named.nameserver.port">53</test_value>
					</test_values>
				</example>
			</examples>
		</rule>
		<rule provider="wernli@in2p3.fr" id='b08c87d1-e44a-4198-8476-b7a610bf13cb'
			class='system.daemon'
			context-id='named'
			context-scope="process">
			<patterns>
				<pattern>connection refused resolving '@ESTRING:system.daemon.named.entry:'@: @IPvANY:system.daemon.named.nameserver.ip@#@NUMBER:system.daemon.named.nameserver.port@</pattern>
			</patterns>
			<examples>
				<example>
					<test_message program="named">connection refused resolving 'my.fqdn.at.tld/AAAA/IN': 10.0.69.163#53</test_message>
					<test_values>
						<test_value name="system.daemon.named.entry">my.fqdn.at.tld/AAAA/IN</test_value>
						<test_value name="system.daemon.named.nameserver.ip">10.0.69.163</test_value>
						<test_value name="system.daemon.named.nameserver.port">53</test_value>
					</test_values>
				</example>
			</examples>
		</rule>
	</rules>
</ruleset>

</patterndb>

